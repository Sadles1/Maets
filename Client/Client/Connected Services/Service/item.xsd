    </xs:sequence>
  </xs:complexType>
  <xs:element name="Profile" nillable="true" type="tns:Profile" />
  <xs:complexType name="Message">
    <xs:sequence>
      <xs:element minOccurs="0" name="IDReceiver" type="xs:int" />
      <xs:element minOccurs="0" name="IDSender" type="xs:int" />
